\def\LlineLRs#1{\@LlineLRs{;#1}}%
\def\@LlineLRs#1{%
  \edef\LR@c{0}%
  \expandafter\@tfor\expandafter\@@c\expandafter:\expandafter=#1\do{%
    \ifnum\LR@c=5\relax
      \if ;\@@c
        \edef\LR@c{0}%
      \else
        \errmessage{LlineLRs: illegal arg}%
      \fi
    \fi
    \ifcase\LR@c
    \or \EMedef\LR@P{\@@c}%
    \or \EMedef\LR@Q{\@@c}%
    \or 
      \EMedef\h@ge{\meaning\@@c}%
      \edef\LR@c@{0}%
      \edef\LR@cmd{}%
      \expandafter\@tfor\expandafter\@c\expandafter:\expandafter=\h@ge\do{%
        \Incr\LR@c@
        \ifnum\LR@c@>9\relax\edefappend\LR@cmd{\@c}\fi
      }%
    \or
      \EMedef\h@ge{\meaning\@@c}%
      \edef\LR@c@{0}%
      \edef\LR@cmd@{}%
      \expandafter\@tfor\expandafter\@c\expandafter:\expandafter=\h@ge\do{%
        \Incr\LR@c@
        \ifnum\LR@c@>9\relax\edefappend\LR@cmd@{\@c}\fi
      }%
      \LlineLR{\LR@P}{\LR@Q}\LR@R\LR@S
      \expandafter\edef\csname \LR@cmd\endcsname{\LR@R}%
      \expandafter\edef\csname \LR@cmd@\endcsname{\LR@S}%
    \fi
    \Incr\LR@c
  }%
}%
